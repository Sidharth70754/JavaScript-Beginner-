function greet() {
    console.log('hello world');
}

greet();



function geet(name) {
    console.log('hello ' + name);
}

geet('sidharth');

// typrs of function 

function square(number) {
    return number * number;
}

let number = square(4);
console.log(number); 

// another way to do this 

function multiplication(number)
{
    return number * number;
}

console.log(multiplication(6));

