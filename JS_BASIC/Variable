let name = 'sidharth';
console.log(name);


// cannot ba a reserved keyword
// should be meaningful
// cannot start with a number (1name)
// cannot contain a space or hypen (-)
// are case sensitive

let firstName  = 'sidharth' ;
let lastName = 'mehta';
console.log(firstName,lastName);

const n = 'sidharth mehta';
 // const is used to declare a variable whose value can not be reassigned
 console.log(n);

